<!DOCTYPE html>
<html lang="en">
<head>
    <title>Document</title>
</head>

<body>
    <%- include('partials/header.ejs') %>
    <h1>
        <%= data.age %>
        <h2>Hobbies</h2>
        <ul>
            <% data.hobbies.forEach(function(item) { %>
